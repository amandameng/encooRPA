                      <mca:CSharpValue x:TypeArguments="x:String" sap2010:WorkflowViewState.IdRef="CSharpValue`1_9" xml:space="preserve">String.Format(@"select o1.order_number from (SELECT count(1) as total_items_count, order_number FROM exported_orders exos
where 
exos.Logistics_warehouse not like '家乐福%店' 
and exos.customer_name = '{0}'
group by exos.order_number) o1 join (SELECT count(1) as canceled_items_count, order_number FROM exported_orders exos
where 
exos.Logistics_warehouse not like '家乐福%店' 
and exos.customer_name = '{1}'
and order_status='已取消'
group by exos.order_number) o2

on o1.order_number = o2.order_number
and o1.total_items_count= o2.canceled_items_count", customer_name, customer_name)</mca:CSharpValue>
